{
  "name": "lnweb-api",
  "version": "0.0.0",
  "description": "LNWeb-API",
  "main": "src/app.js",
  "author": {
    "name": ""
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.16.1",
    "supertest": "^7.0.0"
  },
  "scripts": {
    "start": "node src/app.js",
    "test": "NODE_PATH=./node_modules:./lambda-layer/nodejs/node_modules jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintConfig": {},
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePaths": [
      "<rootDir>/node_modules",
      "<rootDir>/lambda-layer/nodejs/node_modules"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/lambda-layer/nodejs/node_modules"
    ],
    "globals": {
      "process.env.NODE_PATH": "<rootDir>/node_modules:<rootDir>/lambda-layer/nodejs/node_modules"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.936.0",
    "@aws-sdk/client-ssm": "^3.936.0",
    "@aws-sdk/core": "^3.936.0",
    "@aws-sdk/lib-dynamodb": "^3.936.0",
    "@fast-csv/format": "^5.0.5",
    "node-cache": "^5.1.2"
  }
}
